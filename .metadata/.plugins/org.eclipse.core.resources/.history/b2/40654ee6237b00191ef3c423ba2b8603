package topcoder;

public class Fib {
	
	
	public static int fib (int n) {
		
		int result = 0;
		int first = 1;
		int second = 2;
		
		for (int i = 0; i < n - 1; i++) {
			result = first + second;
			//second = first;
			first = second;
			second = result;
		}
		
		return result;
	}
	
	
	
	public static void main (String [] args) {
		
		System.out.println("Fib.java");
		
		int test1 = Fib.fib(3);
		
		
		
		System.out.println("Fib 10 = " + test1);
		
	}

}
